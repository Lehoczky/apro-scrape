{
  "name": "apro-scrape",
  "version": "1.4.0",
  "private": true,
  "description": "Helpful web scraper for hardverapro.hu",
  "author": {
    "name": "Zoltan Lehoczky",
    "email": "ifjlehoczkyzoltan@gmail.com",
    "url": "https://github.com/Lehoczky"
  },
  "main": "./out/main/index.js",
  "scripts": {
    "format:fix": "prettier . --write --ignore-unknown",
    "format:check": "prettier . --check --ignore-unknown",
    "eslint:fix": "eslint . --ext .js,.cjs,.ts,.vue --max-warnings=0 --fix",
    "eslint:check": "eslint . --ext .js,.cjs,.ts,.vue --max-warnings=0",
    "lint": "npm run format:check && npm run eslint:check",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "dev": "electron-vite dev -w",
    "test": "jest",
    "postinstall": "electron-builder install-app-deps",
    "build:renderer": "npm run typecheck && electron-vite build",
    "build:win": "npm run build:renderer && electron-builder --win --config electron-builder.config.cjs",
    "build:linux": "npm run build:renderer && electron-builder --linux --config electron-builder.config.cjs",
    "generate-icons": "electron-icon-builder --input=./resources/icon.png --output=build --flatten",
    "bumpp": "bumpp --commit --tag"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@formkit/auto-animate": "^0.8.2",
    "@vee-validate/zod": "^4.13.1",
    "@vueuse/core": "^10.11.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "custom-electron-titlebar": "^4.2.8",
    "dedent": "^1.5.3",
    "electron-unhandled": "^4.0.1",
    "electron-updater": "^6.1.8",
    "electron-util": "~0.17.2",
    "jsdom": "22.1.0",
    "lucide-vue-next": "^0.395.0",
    "radix-vue": "^1.8.2",
    "tailwind-merge": "^2.3.0",
    "vee-validate": "^4.13.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@lehoczky/eslint-config-typescript": "^1.2.5",
    "@lehoczky/eslint-config-vue": "^1.2.5",
    "@lehoczky/markdownlint-config": "^1.1.4",
    "@lehoczky/prettier-config": "^1.1.6",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.14.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "autoprefixer": "^10.4.19",
    "bumpp": "^9.4.1",
    "electron": "^30.1.1",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-icon-builder": "^2.0.1",
    "electron-vite": "^2.2.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "markdownlint-cli2": "^0.13.0",
    "prettier": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.1.4",
    "typescript": "~5.4.5",
    "vite": "^5.3.1",
    "vue": "^3.4.29",
    "vue-tsc": "^2.0.21"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lehoczky/apro-scrape.git"
  },
  "packageManager": "npm@10.1.0"
}
