{
  "name": "apro-scrape",
  "version": "1.2.4",
  "private": true,
  "description": "Helpful web scraper for hardverapro.hu",
  "author": {
    "name": "Zoltan Lehoczky",
    "email": "ifjlehoczkyzoltan@gmail.com",
    "url": "https://github.com/Lehoczky"
  },
  "main": "./out/main/index.js",
  "scripts": {
    "format:fix": "prettier . --write --ignore-unknown",
    "format:check": "prettier . --check --ignore-unknown",
    "eslint:fix": "eslint . --ext .js,.cjs,.ts,.vue --max-warnings=0 --fix",
    "eslint:check": "eslint . --ext .js,.cjs,.ts,.vue --max-warnings=0",
    "styles:fix": "stylelint \"src/**/*.{css,vue}\" --fix",
    "styles:check": "stylelint \"src/**/*.{css,vue}\"",
    "lint": "npm run format:check && npm run eslint:check && npm run styles:check",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "dev": "electron-vite dev",
    "test": "jest",
    "postinstall": "electron-builder install-app-deps",
    "build:renderer": "npm run typecheck && electron-vite build",
    "build:win": "npm run build:renderer && electron-builder --win --config electron-builder.config.cjs",
    "build:linux": "npm run build:renderer && electron-builder --linux --config electron-builder.config.cjs",
    "generate-icons": "electron-icon-builder --input=./resources/icon.png --output=build --flatten",
    "bumpp": "bumpp --commit --tag --no-push"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.6.8",
    "bootstrap-vue": "^2.23.1",
    "custom-electron-titlebar": "^4.2.8",
    "electron-unhandled": "^4.0.1",
    "electron-updater": "^6.1.8",
    "electron-util": "~0.17.2",
    "jsdom": "22.1.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@lehoczky/eslint-config-typescript": "^1.2.5",
    "@lehoczky/eslint-config-vue": "^1.2.5",
    "@lehoczky/markdownlint-config": "^1.1.4",
    "@lehoczky/prettier-config": "^1.1.6",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.11",
    "@vitejs/plugin-vue2": "^2.3.1",
    "bumpp": "^9.4.1",
    "electron": "^30.0.3",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-icon-builder": "^2.0.1",
    "electron-vite": "^2.2.0",
    "eslint": "^8.57.0",
    "eslint-define-config": "^2.1.0",
    "jest": "^29.7.0",
    "markdownlint-cli2": "^0.13.0",
    "postcss-html": "^1.7.0",
    "prettier": "^3.2.5",
    "stylelint": "^16.5.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "ts-jest": "^29.1.2",
    "typescript": "~5.4.5",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.2.11",
    "vue": "^2.7.16",
    "vue-tsc": "^1.8.27"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lehoczky/apro-scrape.git"
  }
}
