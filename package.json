{
  "name": "apro-scrape",
  "productName": "Apro Scrape",
  "description": "Helpful web scraper for hardverapro.hu",
  "version": "1.1.0",
  "author": {
    "name": "Zoltan Lehoczky",
    "email": "ifjlehoczkyzoltan@gmail.com",
    "url": "https://github.com/Lehoczky"
  },
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service electron:build",
    "start": "vue-cli-service electron:serve",
    "test": "vue-cli-service test:unit --group=unit",
    "e2e": "vue-cli-service test:unit --group=e2e",
    "styles:check": "stylelint \"src/**/*.{css,vue}\"",
    "styles:fix": "stylelint \"src/**/*.{css,vue}\" --fix",
    "format:check": "prettier {src,tests}/** --check",
    "format:fix": "prettier {src,tests}/** --write",
    "generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten"
  },
  "main": "background.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lehoczky/apro-scrape.git"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "bootstrap-vue": "^2.21.2",
    "custom-electron-titlebar": "^4.1.0",
    "electron-unhandled": "^4.0.1",
    "electron-updater": "5.0.1",
    "jsdom": "^16.7.0",
    "vue": "^2.6.14",
    "vue-click-outside": "^1.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-plugin-unit-jest": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "@vue/test-utils": "^1.3.0",
    "electron": "^17.4.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-vue": "^8.6.0",
    "jest-runner-groups": "^2.1.0",
    "spectron": "^11.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "vue-cli-plugin-electron-builder": "^2.1.1",
    "vue-template-compiler": "^2.6.14"
  },
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest/presets/no-babel",
    "runner": "groups",
    "testMatch": [
      "<rootDir>/tests/**/*.spec.(js|ts)"
    ]
  }
}
