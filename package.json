{
  "name": "apro-scrape",
  "productName": "Apro Scrape",
  "description": "Helpful web scraper for hardverapro.hu",
  "version": "1.2.1",
  "author": {
    "name": "Zoltan Lehoczky",
    "email": "ifjlehoczkyzoltan@gmail.com",
    "url": "https://github.com/Lehoczky"
  },
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service electron:build",
    "start": "vue-cli-service electron:serve",
    "test": "vue-cli-service test:unit",
    "styles:check": "stylelint \"src/**/*.{css,vue}\"",
    "styles:fix": "stylelint \"src/**/*.{css,vue}\" --fix",
    "format:check": "prettier {src,tests}/** --check",
    "format:fix": "prettier {src,tests}/** --write",
    "generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lehoczky/apro-scrape.git"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "bootstrap-vue": "^2.23.1",
    "custom-electron-titlebar": "^4.1.0",
    "electron-unhandled": "^4.0.1",
    "electron-updater": "5.3.0",
    "electron-util": "^0.17.2",
    "jsdom": "^16.7.0",
    "vue": "^2.7.13",
    "vue-click-outside": "^1.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/test-utils": "^1.3.0",
    "@vue/vue2-jest": "^27.0.0",
    "electron": "^21.2.2",
    "electron-devtools-installer": "^3.2.0",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.7.0",
    "postcss-html": "^1.5.0",
    "stylelint": "^14.14.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "vue-cli-plugin-electron-builder": "^2.1.1",
    "vue-template-compiler": "^2.7.13"
  },
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest/presets/no-babel",
    "testMatch": [
      "<rootDir>/tests/**/*.spec.(js|ts)"
    ]
  }
}
