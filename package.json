{
  "name": "apro-scrape",
  "version": "1.2.4",
  "private": true,
  "description": "Helpful web scraper for hardverapro.hu",
  "author": {
    "name": "Zoltan Lehoczky",
    "email": "ifjlehoczkyzoltan@gmail.com",
    "url": "https://github.com/Lehoczky"
  },
  "main": "./out/main/index.js",
  "scripts": {
    "format:fix": "prettier . --write --ignore-unknown",
    "format:check": "prettier . --check --ignore-unknown",
    "eslint:fix": "eslint . --ext .js,.cjs,.ts,.vue --max-warnings=0 --fix",
    "eslint:check": "eslint . --ext .js,.cjs,.ts,.vue --max-warnings=0",
    "styles:fix": "stylelint \"src/**/*.{css,vue}\" --fix",
    "styles:check": "stylelint \"src/**/*.{css,vue}\"",
    "lint": "npm run format:check && npm run eslint:check && npm run styles:check",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "dev": "electron-vite dev",
    "postinstall": "electron-builder install-app-deps",
    "build:renderer": "npm run typecheck && electron-vite build",
    "build:win": "npm run build:renderer && electron-builder --win --config electron-builder.config.cjs",
    "build:linux": "npm run build:renderer && electron-builder --linux --config electron-builder.config.cjs",
    "generate-icons": "electron-icon-builder --input=./resources/icon.png --output=build --flatten",
    "bumpp": "bumpp --commit --tag --no-push"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.1",
    "@vueuse/core": "^10.7.0",
    "axios": "^1.6.2",
    "bootstrap-vue": "^2.23.1",
    "custom-electron-titlebar": "^4.2.7",
    "electron-unhandled": "^4.0.1",
    "electron-updater": "^6.1.7",
    "electron-util": "^0.17.2",
    "jsdom": "22.1.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@lehoczky/eslint-config-typescript": "^1.1.0",
    "@lehoczky/eslint-config-vue": "^1.1.0",
    "@lehoczky/markdownlint-config": "^1.1.0",
    "@lehoczky/prettier-config": "^1.1.0",
    "@types/jsdom": "^21.1.5",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue2": "^2.3.1",
    "bumpp": "^9.2.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-icon-builder": "^2.0.1",
    "electron-vite": "^1.0.29",
    "eslint": "^8.55.0",
    "eslint-define-config": "^1.24.1",
    "markdownlint-cli2": "^0.11.0",
    "postcss-html": "^1.5.0",
    "prettier": "^3.1.0",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "~5.1.6",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^4.5.1",
    "vue": "^2.7.15",
    "vue-tsc": "^1.8.25"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lehoczky/apro-scrape.git"
  }
}
